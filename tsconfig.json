{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"declaration": true,
		"declarationDir": "dist/types",
		"emitDeclarationOnly": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"skipLibCheck": true,
		"types": []
	},
	"include": [
		"src"
	]
}